{
  "name": "app-server",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "4.13.3",
    "fs": "0.0.2",
    "multer": "^1.2.1",
    "pg-promise": "^3.2.3",
    "sql.js": "^0.3.2"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-node ./server/server.js",
    "client": "babel-node ./client/start-client.js",
    "api": "nodemon ./server/server.js",
    "test": "newman run server/Tests/uLab.postman_collection.json -e server/Tests/Development.postman_environment.json -n 7 -d server/Tests/list_titles.json",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "newman": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
